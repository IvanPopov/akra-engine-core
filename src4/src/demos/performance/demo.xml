<?xml version="1.0" encoding="utf-8"?>
<Demo Name="Performance" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../lib/grunt/schema.xsd">
  <Description>~800 draw calls per frame</Description>
  <ItemGroup>
    <TypeScriptCompile Include="performance.ts"/>
  </ItemGroup>
  <PropertyGroup>
    <TypeScriptOutFile>../../../$(BuiltDir)/demos/performance/performance.js</TypeScriptOutFile>
    <TypeScriptPropagateEnumConstants>True</TypeScriptPropagateEnumConstants>
    <Dependencies>
      <Module Name="core" />
      <Module Name="addon-progress" />
      <Module Name="addon-base3dObjects" />
    </Dependencies>
    <Resource Name="AE_RESOURCES">
      <Filename>perfomance</Filename>
      <Archive>False</Archive>
      <UseInlining>False</UseInlining>
      <Data>
        <ResourceFile Path="../WoodSoldier/WoodSoldier.DAE" Name="WOODSOLDIER.DAE"/>
        <ResourceFile Path="../cube.DAE" Name="CUBE.DAE"/>
      </Data>
    </Resource>
  </PropertyGroup>
</Demo>
