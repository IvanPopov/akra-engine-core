provide akra.system;
import akra.system.deferredShading;



struct VS_IN{
    float2 position : POSITION;
};

struct VS_OUT{
    float4 position : POSITION;
};

VS_OUT vs_color_maps(VS_IN IN){
	VS_OUT Out;

	
	return Out;
};

float4 fs_color_maps(VS_OUT IN) : COLOR {
    
    return  float4(float3(pixelData.id) / 1000., 1.0);
};	

technique color_maps {
	pass color_maps {
		VertexShader = compile vs_color_maps();
		PixelShader = compile fs_color_maps();
	};
};