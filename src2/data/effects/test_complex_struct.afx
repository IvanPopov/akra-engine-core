struct test_struct_1 {
    float a0:A;
    float2x2 b0[6]:B;
    sampler c0[1]:C;
};

struct test_struct_2 {
    float a1:A;
    float2x2 b1[6]:B;
    sampler c1[2]:C;
    test_struct_1 d1[2]:D;
};


test_struct_1 test_var0[2]:VAR0;
test_struct_2 test_var1[2]:VAR1;


float4 vs_main_test() : POSITION {
    return float4(0.0,0.0,0.0,1.0);
}

float4 fs_main_test() : COLOR {
    float colorY = (test_var0[1]).a0 + test_var1[0].a1;
    float colorZ = tex2D(test_var0[0].c0[0], float2(0.5,0.5)) + tex2D(test_var1[0].c1[1], float2(0.5,0.4)) +
                   tex2D(test_var1[0].d1[1].c0[0], float2(0.5,0.4));
    return float4(1.0,colorY,colorZ,1.0);
}

technique test_technique{
    pass p1{
        VertexShader = compile vs_main_test();
        PixelShader = compile fs_main_test();
    };
};